{
  "name": "@visbot/webvsc-ui",
  "version": "0.8.0",
  "description": "Web frontend for webvsc, a converter for Winamp AVS presets",
  "author": "Jan T. Sott",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production && gulp build",
    "build:dev": "webpack --mode development",
    "test": "eslint src/*.js",
    "precommit": "npm run test",
    "start": "http-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/webvsc-ui.git"
  },
  "dependencies": {
    "@visbot/webvsc": "^2.1.0",
    "file-saver": "^1.3.8",
    "jszip": "^3.1.5"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cssnano": "^3.10.0",
    "eslint": "^4.19.1",
    "gulp": "^4.0.0",
    "gulp-postcss": "^7.0.1",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 5 versions"
            ]
          }
        }
      ]
    ],
    "env": {
      "production": {
        "presets": [
          "minify"
        ]
      }
    }
  },
  "browserslist": "last 5 versions",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "rules": {
      "consistent-return": 0,
      "no-console": 0
    }
  }
}

{
  "name": "@visbot/webvsc-ui",
  "version": "0.8.3",
  "description": "Web frontend for webvsc, a converter for Winamp AVS presets",
  "author": "Jan T. Sott",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && webpack --mode production && gulp build",
    "build:dev": "npm run clean && webpack --mode development",
    "clean": "rimraf ./dist",
    "test": "eslint src/*.js",
    "precommit": "npm run test",
    "start": "http-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/webvsc-ui.git"
  },
  "dependencies": {
    "@visbot/webvsc": "^2.2.1",
    "file-saver": "^1.3.8",
    "jszip": "^3.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.2",
    "cssnano": "^4.1.3",
    "eslint": "^5.6.0",
    "gulp": "^4.0.0",
    "gulp-postcss": "^8.0.0",
    "http-server": "^0.11.1",
    "husky": "^1.0.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 5 versions"
            ]
          }
        }
      ]
    ],
    "env": {
      "production": {
        "presets": [
          "minify"
        ]
      }
    }
  },
  "browserslist": "last 5 versions",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "rules": {
      "consistent-return": 0,
      "no-console": 0
    }
  }
}
